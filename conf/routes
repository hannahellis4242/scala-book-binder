GET     /                           controllers.StartController.get()
POST    /                           controllers.StartController.post()

GET     /pages                      controllers.PagesController.get()
POST    /pages                      controllers.PagesController.post()

GET     /max                        controllers.MaxController.get()
POST    /max                        controllers.MaxController.post()

GET     /signatures                 controllers.SignaturesController.get()
POST    /signatures                 controllers.SignaturesController.post()

GET     /options                    controllers.OptionsController.get()
POST    /options                    controllers.OptionsController.post()

GET     /sequence                   controllers.SequenceController.get()
POST    /sequence                   controllers.SequenceController.post()

GET     /report                     controllers.ReportController.get()

GET     /error                      controllers.ErrorController.get(message)

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
